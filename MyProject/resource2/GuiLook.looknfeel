                <Area>
                    <Dim type="LeftEdge" >
                        <ImageDim imageset="GuiLook" image="HeaderBarSortUp" dimension="Width">
                            <DimOperator op="Add">
                                <AbsoluteDim value="5" />
                            </DimOperator>
                        </ImageDim>
                    </Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
