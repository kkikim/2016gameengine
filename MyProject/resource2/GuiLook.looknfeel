            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="3" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="3" /></Dim>
                    <Dim type="Width" ><ImageDim imageset="GuiLook" image="HeaderBarSortUp" dimension="Width" /></Dim>
                    <Dim type="Height" ><ImageDim imageset="GuiLook" image="HeaderBarSortUp" dimension="Height" /></Dim>
                </Area>
                <Image imageset="GuiLook" image="HeaderBarSortUp" />
            </ImageryComponent>
        </ImagerySection>
        <ImagerySection name="descend_icon">
